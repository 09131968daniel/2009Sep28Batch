package com.example.datatypes;

public class DataTypes {
	
	/*
	 * java has two different types, primitive and reference
	 * 
	 * primitives:
	 * 		byte
	 * 		boolean
	 * 		char
	 * 		float
	 * 		double
	 * 		int           32 bits
	 * 		short
	 * 		long
	 * 
	 * 
	 * 
	 * 
	 */

	
	public static void main(String[] args) {
		//declaring a variable, datatype name == value;
		
		int x = 7;
		System.out.println(x);
	}
}
