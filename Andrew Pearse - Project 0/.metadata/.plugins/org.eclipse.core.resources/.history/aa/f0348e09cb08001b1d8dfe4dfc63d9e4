package com.dealership.models;

public class Car {

	private String make;
	private String model;
	private int year;
	private int price;
	private int[] offers;

	public Car(String make, String model, int price, int year, int[] offers) {
		this.make = make;
		this.model = model;
		this.price = price;
		this.year = year;
		this.offers = offers;
	}

	public String getMake() {
		return make;
	}

	public void setMake(String make) {
		this.make = make;
	}

	public String getModel() {
		return model;
	}

	public void setModel(String model) {
		this.model = model;
	}

	public int getPrice() {
		return price;
	}

	public void setPrice(int price) {
		this.price = price;
	}
	
	public int getYear() {
		return year;
	}

	public void setYear(int year) {
		this.year = year;
	}

	@Override
	public String toString() {
		return year + make +  model + "\nprice=" + price + "\n";
	}

	public int[] getOffers() {
		return offers;
	}

	public void setOffers(int[] offers) {
		this.offers = offers;
	}
	
}
